{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "../lib",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "lib": [
          "es2015"
        ]
    },
    "include": [
        "./**/*.ts"
    ],
    "atom": {
        "rewriteTsconfig": false
    }
}
